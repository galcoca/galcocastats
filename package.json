{
  "name": "galcocastats",
  "version": "1.0.1",
  "packageManager": "bun@1.3.6",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "check-outdated": "./scripts/check-outdated.sh",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0 && echo '✅ Lint passed'",
    "lint:fix": "eslint . --ext .ts,.tsx --fix && eslint . --ext .ts,.tsx --max-warnings 0 && echo '✅ Lint fixed'",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@vercel/og": "^0.8.6",
    "dayjs": "^1.11.19",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "satori": "^0.19.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/bun": "^1.3.8",
    "@types/node": "^24",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=24"
  }
}
